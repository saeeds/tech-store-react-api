{
  "connection": "default",
  "collectionName": "products",
  "info": {
    "name": "product"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "description": {
      "type": "text"
    },
    "featured": {
      "type": "boolean"
    },
    "category": {
      "type": "string"
    },
    "Free_shipping": {
      "type": "boolean"
    },
    "extra_field": {
      "type": "string"
    }
  }
}
